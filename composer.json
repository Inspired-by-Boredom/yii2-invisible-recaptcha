{
  "name": "vintage/yii2-invisible-recaptcha",
  "description": "Facade of Invisible reCAPTCHA by Google for Yii2 Framework",
  "keywords": ["yii2", "widget", "invisible", "captcha", "re-captcha", "google"],
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "support": {
    "source": "https://github.com/vintage/yii2-invisible-recaptcha",
    "issues": "https://github.com/vintage/yii2-invisible-recaptcha/issues"
  },
  "authors": [
    {
      "name": "Vladimir Kuprienko",
      "email": "vldmr.kuprienko@gmail.com",
      "role": "developer"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0.0",
    "yiisoft/yii2-httpclient": "~2.0.0"
  },
  "require-dev": {
    "codeception/codeception": "~2.2",
    "codeception/verify": "*",
    "codeception/specify": "*"
  },
  "autoload": {
    "psr-4": {
      "vintage\\recaptcha\\": "src"
    }
  },
  "autoload-dev": {
    "prs-4": {
      "vintage\\recaptcha\\tests\\": "tests"
    }
  }
}